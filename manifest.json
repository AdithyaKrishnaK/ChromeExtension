{
  "manifest_version": 2,

 
 "name": "Dock",
  
"description": "This extension enables user to access webpages quickly",
  
"version": "1.0",

 
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides":{"newtab":""}
}
