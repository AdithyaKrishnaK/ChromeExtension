{
  "manifest_version": 2,

 "name": "Dock",
  
"description": "This extension enables user to access webpages quickly",
  
"version": "1.0","content_scripts": [
    {
      "matches": ["*"],
      "js": ["jquery.js", "p.js"]
    }
  ],
 
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides":{"newtab":"dock.html"}
}
